"pamixer --increase 5"
    XF86AudioRaiseVolume

"pamixer --decrease 5"
    XF86AudioLowerVolume

"pulseaudio-ctl mute"
    XF86AudioMute

"pamixer --increase 5"
    Mod4 + b:4

"pamixer --decrease 5"
    Mod4 + b:5

"mpc toggle -h localhost"
    Mod4 + comma

"fzf_run"
    Mod4 + d

